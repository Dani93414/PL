x := 'tomate';

print 'Prueba de funcionamiento del switch:\n\n';

switch (x)
  case 'patata':
    print 'Recibido patata\n';
  case 'tomate':
    print 'Recibido tomate\n';
  default:
    print 'Recibido otro valor\n';
end_switch


